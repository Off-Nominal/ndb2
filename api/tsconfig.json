{
  "compilerOptions": {
    "target": "es2015",
    "module": "Node16",
    "sourceMap": true,
    "outDir": "dist",
    "lib": ["es2021.string"],
    "downlevelIteration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true
  },
  "include": ["src/**/*", "db/empty.js"],
  "exclude": ["node_modules"]
}
