{
  "name": "ndb2",
  "version": "2.1.0",
  "description": "Nostradambot 2.0 Betting Game for the Off-Nominal Discord",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --filter ndb2-api dev",
    "start": "pnpm --filter ndb2-api start",
    "build": "pnpm --filter ndb2-api build",
    "compile": "pnpm --filter ndb2-api compile",
    "test": "pnpm --filter ndb2-api test",
    "test:db:start": "cd docker/test-database && ./start-test-db.sh",
    "test:db:stop": "cd docker/test-database && ./stop-test-db.sh",
    "test:db:reset": "cd docker/test-database && ./reset-test-db.sh",
    "test:db": "vitest --run src/**/*.db.test.ts"
  },
  "bugs": {
    "url": "https://github.com/mendahu/ndb2/issues"
  },
  "author": "Jake Robins",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mendahu/ndb2.git"
  },
  "homepage": "https://github.com/mendahu/ndb2#readme"
}
